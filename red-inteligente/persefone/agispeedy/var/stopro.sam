            $fp_accessPrice1= 0;
            $fp_sampDelayInSec1 = 0;
            $fp_sampPrice1 = 0;
            $fp_nmaxSamples1= 0;
            $fp_accessPrice2 = 0;
            $fp_sampDelayInSec2= 0;
            $fp_sampPrice2= 0;
            $fp_nmaxSamples2=0;
            $fp_accessPrice3=0;
