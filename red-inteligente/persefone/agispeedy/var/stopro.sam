            $fp_accessPrice4=0;
            $fp_sampDelayInSec4 = 0;
            $fp_sampPrice4= 0;
            $fp_nmaxSamples4=0;
            $fp_accessPrice5= 0;
            $fp_sampDelayInSec5= 0;
            $fp_sampPrice5 = 0;
            $fp_nmaxSamples5= 0;
            $fp_accessPrice6 = 0;
            $fp_sampDelayInSec6 = 0;
            $fp_sampPrice6= 0;
            $fp_nmaxSamples6 = 0;
            $fp_accessPrice7 = 0;
