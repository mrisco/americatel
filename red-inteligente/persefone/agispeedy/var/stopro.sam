            $sth->bind_param(45, $fp_call_out_outgoingLocation_channelNumber, SQL_INTEGER);
            $sth->bind_param(46, $dni_canonical, SQL_CHAR);
            $sth->bind_param(47, $fp_call_out_dialingDurationInSec, SQL_INTEGER);
