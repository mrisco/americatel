use DBI;
use DBI qw(:sql_types);
use POSIX qw/strftime/;

$dbh = null;
$dba = null;

sub stopro {
   my $self = shift;
   my $server_prop = $self->{server};
   my $agi = $server_prop->{agi};
   my $params = $agi->{params};
   $dbh = $self->{server}{dbh};
   $dba = $self->{server}{dba};
   $dbb = $self->{server}{dbb};

   my $agent = $params->{'agent'};
   my $billing = $params->{'billing'};

   $dbh->do("use HOST"); 
   $dba->do("use $agent"); 
   $dbb->do("use $billing"); 

